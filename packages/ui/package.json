{
	"name": "@crossbell/ui",
	"version": "1.5.11",
	"license": "MIT",
	"repository": "github:Crossbell-Box/crossbell-universe",
	"bugs": "https://github.com/Crossbell-Box/crossbell-universe/issues",
	"homepage": "https://github.com/Crossbell-Box/crossbell-universe#readme",
	"author": "Natural Selection Labs",
	"keywords": [],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"typings": "dist/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"build": "run-s clean build:icons build:ts",
		"build:icons": "svgr --typescript --ref --icon --filename-case kebab --index-template svgr-index-template.js --out-dir src/icons/svgr  ../../scripts/icons",
		"build:ts": "tsup",
		"clean": "rm -rf ./dist && rm -rf ./src/icons/svgr"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@crossbell/indexer": "^1.5.11",
		"@crossbell/util-hooks": "^1.5.11",
		"@crossbell/util-metadata": "^1.5.11",
		"@tanstack/react-query": "^4",
		"classnames": "^2",
		"crossbell": "^1.6.4",
		"facepaint": "^1.2.1",
		"react-intersection-observer": "^9.5.2",
		"react-remove-scroll": "^2.5.6",
		"react-transition-group": "^4.4.5",
		"ts-essentials": "^9.4.0"
	},
	"peerDependencies": {
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@svgr/cli": "8.1.0",
		"pascal-case": "^3.1.2"
	}
}
