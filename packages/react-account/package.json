	"homepage": "https://github.com/Crossbell-Box/crossbell-universe#readme",
	"author": "Natural Selection Labs",
	"keywords": [],
	"main": "dist/index.js",
	"module": "dist/ts/index.js",
	"typings": "dist/ts/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist",
		"apis",
		"modal-config",
		"storage-config",
		"ssr-config",
		"utils",
		"package.json"
	],
	"scripts": {
		"build": "tsup",
		"clean": "rm -rf ./dist"
	},
	"exports": {
		".": {
			"types": "./dist/ts/index.d.ts",
			"import": "./dist/ts/index.js",
			"require": "./dist/index.js"
		},
		"./modal-config": {
			"types": "./dist/ts/modal-config/index.d.ts",
			"import": "./dist/ts/modal-config/index.js",
			"require": "./dist/modal-config.js"
		},
		"./storage-config": {
			"types": "./dist/ts/storage-config/index.d.ts",
			"import": "./dist/ts/storage-config/index.js",
			"require": "./dist/storage-config.js"
		},
		"./ssr-config": {
			"types": "./dist/ts/ssr-config/index.d.ts",
			"import": "./dist/ts/ssr-config/index.js",
			"require": "./dist/ssr-config.js"
		},
		"./utils": {
			"types": "./dist/ts/utils/index.d.ts",
			"import": "./dist/ts/utils/index.ts",
			"require": "./dist/utils.js"
		},
		"./apis": {
			"types": "./dist/ts/apis/index.d.ts",
			"import": "./dist/ts/apis/index.js",
			"require": "./dist/apis.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@crossbell/contract": "^1.4.5",
		"@crossbell/indexer": "^1.4.5",
		"@crossbell/util-ethers": "^1.4.0",
		"@crossbell/util-hooks": "^1.4.4",
		"@crossbell/util-metadata": "^1.4.0",
		"check-password-strength": "^2",
		"crossbell": "^1.4.0",
		"immer": "^10",
		"lodash.debounce": "4.0.8",
		"react": "^18",
		"rxjs": "^7.8.1",
		"ts-essentials": "^9.3.2",
		"viem": "^1.1.6",
		"zustand": "^4"
	},
	"peerDependencies": {
		"@tanstack/react-query": "^4"
	},
	"devDependencies": {
		"@types/lodash.debounce": "4.0.7"
	}
